<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<template>
		<name>Custom Artist Tags</name>
		<description>Menu that makes it possible to browse your artists by a custom tag, the artist is selected first and then the custom tag, for example ORIGARTIST, requires the Custom Scan plugin</description>
		<parameter type="text" id="menuname" name="Menu name" value="Custom Artist Tags"/>
		<parameter type="text" id="menuorder" name="Menu sort order (1-100)" value="50"/>
		<parameter type="checkbox" id="variousartists" name="Various Artists section" value=""/>
		<parameter type="text" id="variousartisttext" name="Various Artists name" value="Various Artists"/>
		<parameter type="text" id="customtag" name="Tag name" value="ORIGARTIST"/>
		<parameter type="text" id="customtagtext" name="User friendly name of tag" value="Orig Artists"/>
		<parameter type="text" id="customsorttag" name="Sort tag name" value=""/>
		<parameter type="text" id="includedclients" name="Only include on these players (empty = all)" value=""/>
		<parameter type="text" id="excludedclients" name="Exclude from these players" value=""/>
	</template>
</custombrowse>

